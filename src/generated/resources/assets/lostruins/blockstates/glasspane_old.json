{
  "multipart": [
    {
      "apply": {
        "model": "lostruins:block/glasspane_old_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "lostruins:block/glasspane_old_noside_alt"
      }
    }
  ]
}